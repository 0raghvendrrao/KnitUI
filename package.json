{
  "name": "Knit-UI",
  "version": "0.1.0",
  "description": "Component library for Clarisights",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "git@github.com:adwyze/Knit-UI.git",
  "author": "Rohan Malhotra <rohan.root@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rm -rf lib && rm -rf es",
    "build": "npm-run-all --parallel build:cjs build:es",
    "build:cjs": "NODE_ENV=production babel components --out-dir lib --copy-files --ignore __tests__,spec.jsx,test.jsx,__snapshots__",
    "build:es": "NODE_ENV=production BABEL_ENV=es babel components --out-dir es --copy-files --ignore __tests__,spec.jsx,test.jsx,__snapshots__",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.3.4",
    "@storybook/addon-info": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@types/jest": "^24.0.9",
    "@types/storybook__react": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.4",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.8.4",
    "ts-jest": "^24.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  }
}
